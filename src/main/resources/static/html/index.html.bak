    <section id="contact" class="content-section">
        <div id="map">

            <!-- How to change your own map point
                   1. Go to Google Maps
                   2. Click on your location point
                   3. Click "Share" and choose "Embed map" tab
                   4. Copy only URL and paste it within the src="" field below
            -->
        </div>
        <div id="contact-content">
            <div class="section-heading">
                <h1>博客<br><em>随笔</em></h1>
                <p>水滴石穿
                    <br></p>

            </div>
            <div class="section-content">
                <form id="contact" method="post">
                    <div class="row">
                        <div class="col-md-4">
                            <fieldset>
                                <input name="blogName" type="text" class="form-control" id="blogName" placeholder="Your Blog Name..."
                                       required="">
                            </fieldset>
                        </div>
                        <div class="col-md-4">
                            <fieldset>
                                <select id="blogTypeSelect" class="form-control">
                                    <option value="0" >请选择博客类型</option>
                                    <option value="1" >SE</option>
                                    <option value="2" >线程</option>
                                    <option value="3" >IO</option>
                                    <option value="4" >反射</option>
                                    <option value="5" >SSM</option>
                                </select>
                                <!--<input name="blogType" type="email" class="form-control" id="email"
                                       placeholder="Your email..." required="">-->
                            </fieldset>
                        </div>
                        <!--<div class="col-md-4">
                            <fieldset>
                                <input name="subject" type="text" class="form-control" id="subject"
                                       placeholder="Subject..." required="">
                            </fieldset>
                        </div>-->
                        <div class="col-md-12">
                            <fieldset>
                                <textarea name="blogIntroduce" rows="6" class="form-control" id="blogIntroduce"
                                          placeholder="Blog Introduce..." required></textarea>
                            </fieldset>
                        </div>
                        <div class="col-md-12">
                            <fieldset>
                                <button id="blogSubmit" class="btn" @click.prevent="openMdEdit">编辑</button>
                            </fieldset>
                        </div>
                    </div>
                </form>
            </div>
        </div>
